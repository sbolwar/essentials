    <validationRules>
        <fullName>Dont_Allow_Numbers_In_Candidate_Name</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISNUMBER(Candidate_Name__c), CONTAINS(Candidate_Name__c , &apos;%&apos;))</errorConditionFormula>
        <errorDisplayField>Candidate_Name__c</errorDisplayField>
        <errorMessage>Numbers are not allowed in Candidate Name</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Make_Description_Mandatory_On_Other_Reas</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
   ISPICKVAL( Candidate_Reason__c, &apos;Other&apos;),
   ISBLANK( Description__c )
   )</errorConditionFormula>
        <errorDisplayField>Description__c</errorDisplayField>
        <errorMessage>The description is mandatory for Candidate reason &apos;Other&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Only_Numbers_For_Age</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISNUMBER(Candidate_Complete_Age__c))</errorConditionFormula>
        <errorMessage>Only numbers are allowed in age.</errorMessage>
    </validationRules>
